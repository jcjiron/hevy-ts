{
  "name": "hevy-ts",
  "version": "1.0.0",
  "description": "A TypeScript client for the Hevy API",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "keywords": [
    "hevy",
    "typescript",
    "api",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jcjiron/hevy-ts.git"
  },
  "scripts": {
    "test": "npm run build && jest",
    "test:watch": "npm run build && jest --watch",
    "test:coverage": "jest --coverage",
    "build": "rimraf ./dist && tsc",
    "start": "npm run build && node dist/src/app.js",
    "dev": "nodemon src/app.ts"
  },
  "author": "Juan Carlos JirÃ³n",
  "license": "ISC",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "dotenv": "^16.4.7",
    "jest": "^29.7.0",
    "jest-mock-axios": "^4.8.0",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "winston": "^3.17.0"
  },
  "files": [
    "dist"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer"
      ],
      [
        "@semantic-release/release-notes-generator"
      ],
      [
        "@semantic-release/changelog"
      ],
      [
        "@semantic-release/npm"
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ðŸš€ ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "bugs": {
    "url": "https://github.com/jcjiron/hevy-ts/issues"
  },
  "homepage": "https://github.com/jcjiron/hevy-ts#readme"
}
